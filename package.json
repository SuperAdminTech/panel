{
  "name": "trading-bots-panel",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --codeCoverage --browsers ChromeHeadless",
    "test-ci": "ng test --codeCoverage --karma-config=./karma-ci.conf.js --no-progress --browsers ChromeHeadless",

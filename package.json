    "protractor": "~5.4.0",
    "puppeteer": "^3.3.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "yargs": "^15.3.1",

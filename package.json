{
  "name": "trading-bots-panel",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --codeCoverage --browsers ChromeHeadless",
    "test-ci": "ng test --codeCoverage --karma-config=./karma-ci.conf.js --no-progress --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test-with-coverage": "ng test --codeCoverage --noWatch --browsers ChromeHeadless",
    "badges": "node scripts/coverage-badges.js",
    "push": "npm run badges && git commit --amend -an && git push",
    "version": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "~10.2.6",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@qbitartifacts/caste-client-ng": "^1.2.1",
    "@qbitartifacts/qbit-kit-ng": "0.0.24",
    "bootstrap-grid-only-css": "^4.1.3",
    "directive": "^0.1.0",
    "hammerjs": "^2.0.8",
    "highcharts": "^8.2.2",
    "husky": "^4.3.0",
    "jwt-decode": "^3.0.0",
    "karma-coverage": "^2.0.3",
    "moment": "^2.29.1",
    "ngx-avatar": "^4.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/language-service": "~10.2.0",
    "@netlify-builder/deploy": "~3.1.0",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.14.5",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.0.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.2",
    "protractor": "~7.0.0",
    "puppeteer": "^5.5.0",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "yargs": "^16.1.0",
    "standard-version": "^9.0.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-with-coverage"
    }
  }
